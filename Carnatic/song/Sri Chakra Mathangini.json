\u0c28\u0c28\u0c3f <br/> \u0c32\u0c32\u0c3e\u0c2e \u0c2e\u0c30\u0c15\u0c24 \u0c32\u0c3e\u0c32\u0c2f\u0c24 \u0c32\u0c32\u0c3e\u0c1f \u0c38\u0c3f\u0c28\u0c4d\u0c27\u0c42\u0c30 \u0c24\u0c3f\u0c32\u0c15\u0c47 \u0c32\u0c17\u0c41\u0c27\u0c30 <br/> \u0c38\u0c30\u0c4d\u0c35\u0c3e\u0c30\u0c4d\u0c17\u0c2a\u0c35\u0c30\u0c4d\u0c17 \u0c38\u0c41\u0c16\u0c26\u0c3e\u0c2f\u0c3f\u0c28\u0c47 \u0c38\u0c41\u0c2e\u0c24\u0c47 \u0c2e\u0c39\u0c3e\u0c26\u0c47\u0c35\u0c3f \u0c38\u0c30\u0c38\u0c3f\u0c1c\u0c3e\u0c15\u0c4d\u0c37\u0c3f"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0c9a\u0c95\u0ccd\u0cb0 \u0cae\u0cbe\u0ca4\u0c82\u0c97\u0cbf\u0ca8\u0cc0 \u0ca6\u0cc7\u0cb5\u0ca6\u0cc7\u0cb5 \u0cb6\u0cc8\u0cb2\u0cc7\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0ca8\u0ca8\u0ccd\u0ca6\u0cbf\u0ca8\u0cbf \u0cb6\u0cbf\u0cb5\u0cad\u0cbe\u0c97\u0cbe \u0ca8\u0ca8\u0ccd\u0ca6\u0cbf\u0ca8\u0cc0\u0c95\u0cbe\u0ca8\u0ccd\u0ca4 <br/> \u0ca8\u0cbf\u0cb0\u0ca8\u0ccd\u0ca4\u0cb0 \u0c95\u0ca6\u0cae\u0ccd\u0cac\u0cb5\u0ca8 \u0cb0\u0c82\u0c9c\u0ca8\u0cbf \u0c95\u0cbe\u0cae\u0c95\u0ccb\u0c9f\u0cbf \u0c95\u0cb2\u0cbe\u0caa\u0cc0\u0ca0 \u0cb5\u0cbf\u0cb0\u0cbe\u0c9c\u0cbf\u0ca8\u0cbf"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cad\u0cc2 \u0c9a\u0c95\u0ccd\u0cb0 \u0cae\u0cc7\u0cb0\u0cc1 \u0ca8\u0cbf\u0c95\u0cc7\u0ca4\u0ca8\u0cc7 \u0cad\u0cc1\u0cb5\u0ca8\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf \u0cb9\u0cae\u0ccd\u0cb8\u0c97\u0cae\u0ca8\u0cc7 \u0caa\u0cc1\u0cb0\u0cc1\u0cb9\u0cc2\u0ca4 <br/> \u0c9c\u0cbe\u0caf\u0cbe \u0caa\u0cc2\u0c9c\u0cbf\u0ca4 \u0c9a\u0cb0\u0ca3\u0cc7 \u0caa\u0cc1\u0cb2\u0ccd\u0cb2\u0cbe\u0cb0\u0cb5\u0cbf\u0ca8\u0ccd\u0ca6 \u0ca8\u0caf\u0ca8\u0cc7 \u0cae\u0cbe\u0cae\u0cb5"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0cad\u0cb5\u0cbe\u0ca8\u0cbf \u0cae\u0ca7\u0cc1\u0cb0\u0cb5\u0cbe\u0ca3\u0cbf \u0cb8\u0cc1\u0c96\u0cb5\u0cbe\u0ca3\u0cbf \u0ca8\u0cc1\u0ca4\u0cb0\u0cae\u0cbe\u0cb5\u0cbe\u0ca3\u0cbf\u0caa\u0cbe\u0cb6\u0cbe\u0c82\u0c95\u0ccd\u0cb6-"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca7\u0cb0\u0ca3\u0cc7 \u0c95\u0cb2\u0cbe\u0cb5\u0cbf\u0c95\u0cb2\u0cbe \u0c95\u0cb0\u0c95\u0cae\u0ccd\u0caa\u0cbf\u0ca4 \u0cb2\u0ccb\u0cb2\u0cbe \u0c9b\u0cbe\u0cae\u0cb0\u0cc0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0caa\u0c82\u0c9a\u0cbe\u0caf\u0ca4\u0ca8 \u0cb8\u0ca8\u0cbe\u0ca4\u0ca8 \u0caa\u0cc2\u0c9c\u0ca8 \u0caa\u0cbe\u0cb5\u0ca8 \u0cad\u0cbe\u0cb5\u0ca8\u0cbe\u0c95\u0ccd\u0cb7\u0cb0\u0cbf \u0cb2\u0ccb\u0cb2\u0ca8 <br/> \u0caa\u0ca6\u0ccd\u0cae\u0cbe\u0cb8\u0ca8 \u0ca7\u0ccd\u0caf\u0cbe\u0ca8 \u0cae\u0ca8\u0ca8 \u0ca7\u0cbe\u0cb0\u0ca3 \u0cb8\u0cc7\u0cb5\u0ca8 \u0ca7\u0cc0\u0cb0 \u0ca8\u0cbf\u0cb0\u0ca8\u0ccd\u0ca4\u0cb0 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c95\u0cae\u0cb2\u0cbe\u0caa\u0cc1\u0cb0 \u0cb8\u0ca6\u0ca8\u0cc7 \u0c95\u0cae\u0ca8\u0cc0\u0caf \u0cb5\u0ca6\u0ca8\u0cc7 \u0c95\u0cb0\u0cc1\u0ca3\u0cbe\u0caf\u0ca4 \u0ca8\u0caf\u0ca8\u0cc7 \u0c95\u0cb0 <br/> \u0c95\u0c82\u0c9c\u0ca8\u0cc7\u0c95\u0ccd\u0cb7\u0cc1 \u0ca7\u0ca8\u0cc1\u0cb0\u0cbe\u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0 \u0cae\u0ca7\u0cc1\u0c95\u0cc8\u0c9f\u0cad \u0cb8\u0ca6\u0ca8\u0cc7 <br/> \u0c97\u0c82\u0c97\u0cbe \u0cad\u0cc1\u0c9c\u0c82\u0c97 \u0c95\u0cae\u0cb2 \u0cb5\u0cc8\u0cb0\u0cbf\u0ca4\u0cb0 \u0c9c\u0c9f\u0cbe \u0cae\u0c95\u0cc1\u0c9f \u0c95\u0cb2\u0ccd\u0cb2\u0ccb\u0cb2 \u0cb5\u0cbf\u0cb2\u0ccb\u0c95\u0ccd\u0caf <br/> \u0c95\u0cb0\u0cbe\u0ca7\u0cc1\u0cb0\u0cc1\u0c95\u0ccd\u0cb0\u0cae \u0cb2\u0cc0\u0cb2\u0cc7 \u0ca8\u0cbf\u0ca6\u0cc1\u0cb5\u0ca8 \u0cb6\u0cc0\u0cb2\u0cc7 \u0cb8 \u0ca8\u0cbf\u0c97\u0cae \u0cb2\u0cc0\u0cb2\u0cc7 \u0c9c\u0caf \u0c9c\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cb2\u0cb2\u0cbf\u0ca4\u0cc7 \u0cb2\u0cbe\u0cb5\u0ca3\u0ccd\u0caf \u0cb8\u0cbe\u0cb0\u0cae\u0c82\u0c97\u0cb3 \u0cb5\u0cbf\u0c97\u0ccd\u0cb0\u0cb9\u0cc7 \u0c95\u0cb2\u0ccd\u0caa\u0cb2\u0ca4\u0cbf\u0c95\u0cc7 \u0cb2\u0c95\u0cbe\u0cb0 <br/> \u0cb2\u0cbe\u0cb2\u0ca8 \u0ca4\u0ca4\u0ccd\u0cb5 \u0ca8\u0cbf\u0cb0\u0cbe\u0cae\u0caf\u0cbf \u0c9a\u0cbf\u0ca8\u0ccd\u0cae\u0caf\u0cbf \u0cb2\u0cae\u0ccd\u0cac\u0ccb\u0ca7\u0cb0 \u0c97\u0cc1\u0cb0\u0cc1\u0c97\u0cc1\u0cb9 \u0c9c\u0ca8\u0ca8\u0cbf <br/> \u0cb2\u0cb2\u0cbe\u0cae \u0cae\u0cb0\u0c95\u0ca4 \u0cb2\u0cbe\u0cb2\u0caf\u0ca4 \u0cb2\u0cb2\u0cbe\u0c9f \u0cb8\u0cbf\u0ca8\u0ccd\u0ca7\u0cc2\u0cb0 \u0ca4\u0cbf\u0cb2\u0c95\u0cc7 \u0cb2\u0c97\u0cc1\u0ca7\u0cb0 <br/> \u0cb8\u0cb0\u0ccd\u0cb5\u0cbe\u0cb0\u0ccd\u0c97\u0caa\u0cb5\u0cb0\u0ccd\u0c97 \u0cb8\u0cc1\u0c96\u0ca6\u0cbe\u0caf\u0cbf\u0ca8\u0cc7 \u0cb8\u0cc1\u0cae\u0ca4\u0cc7 \u0cae\u0cb9\u0cbe\u0ca6\u0cc7\u0cb5\u0cbf \u0cb8\u0cb0\u0cb8\u0cbf\u0c9c\u0cbe\u0c95\u0ccd\u0cb7\u0cbf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40 \u0d1a\u0d15\u0d4d\u0d30 \u0d2e\u0d3e\u0d24\u0d02\u0d17\u0d3f\u0d28\u0d40 \u0d26\u0d47\u0d35\u0d26\u0d47\u0d35 \u0d36\u0d48\u0d32\u0d47\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d28\u0d28\u0d4d\u0d26\u0d3f\u0d28\u0d3f \u0d36\u0d3f\u0d35\u0d2d\u0d3e\u0d17\u0d3e \u0d28\u0d28\u0d4d\u0d26\u0d3f\u0d28\u0d40\u0d15\u0d3e\u0d28\u0d4d\u0d24 <br/> \u0d28\u0d3f\u0d30\u0d28\u0d4d\u0d24\u0d30 \u0d15\u0d26\u0d2e\u0d4d\u0d2c\u0d35\u0d28 \u0d30\u0d02\u0d1c\u0d28\u0d3f \u0d15\u0d3e\u0d2e\u0d15\u0d4b\u0d1f\u0d3f \u0d15\u0d32\u0d3e\u0d2a\u0d40\u0d20 \u0d35\u0d3f\u0d30\u0d3e\u0d1c\u0d3f\u0d28\u0d3f"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2d\u0d42 \u0d1a\u0d15\u0d4d\u0d30 \u0d2e\u0d47\u0d30\u0d41 \u0d28\u0d3f\u0d15\u0d47\u0d24\u0d28\u0d47 \u0d2d\u0d41\u0d35\u0d28\u0d47\u0d36\u0d4d\u0d35\u0d30\u0d3f \u0d39\u0d2e\u0d4d\u0d38\u0d17\u0d2e\u0d28\u0d47 \u0d2a\u0d41\u0d30\u0d41\u0d39\u0d42\u0d24 <br/> \u0d1c\u0d3e\u0d2f\u0d3e \u0d2a\u0d42\u0d1c\u0d3f\u0d24 \u0d1a\u0d30\u0d23\u0d47 \u0d2a\u0d41\u0d32\u0d4d\u0d32\u0d3e\u0d30\u0d35\u0d3f\u0d28\u0d4d\u0d26 \u0d28\u0d2f\u0d28\u0d47 \u0d2e\u0d3e\u0d2e\u0d35"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2d\u0d35\u0d3e\u0d28\u0d3f \u0d2e\u0d27\u0d41\u0d30\u0d35\u0d3e\u0d23\u0d3f \u0d38\u0d41\u0d16\u0d35\u0d3e\u0d23\u0d3f \u0d28\u0d41\u0d24\u0d30\u0d2e\u0d3e\u0d35\u0d3e\u0d23\u0d3f\u0d2a\u0d3e\u0d36\u0d3e\u0d02\u0d15\u0d4d\u0d36-"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d27\u0d30\u0d23\u0d47 \u0d15\u0d32\u0d3e\u0d35\u0d3f\u0d15\u0d32\u0d3e \u0d15\u0d30\u0d15\u0d2e\u0d4d\u0d2a\u0d3f\u0d24 \u0d32\u0d4b\u0d32\u0d3e \u0d1b\u0d3e\u0d2e\u0d30\u0d40"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d2a\u0d02\u0d1a\u0d3e\u0d2f\u0d24\u0d28 \u0d38\u0d28\u0d3e\u0d24\u0d28 \u0d2a\u0d42\u0d1c\u0d28 \u0d2a\u0d3e\u0d35\u0d28 \u0d2d\u0d3e\u0d35\u0d28\u0d3e\u0d15\u0d4d\u0d37\u0d30\u0d3f \u0d32\u0d4b\u0d32\u0d28 <br/> \u0d2a\u0d26\u0d4d\u0d2e\u0d3e\u0d38\u0d28 \u0d27\u0d4d\u0d2f\u0d3e\u0d28 \u0d2e\u0d28\u0d28 \u0d27\u0d3e\u0d30\u0d23 \u0d38\u0d47\u0d35\u0d28 \u0d27\u0d40\u0d30 \u0d28\u0d3f\u0d30\u0d28\u0d4d\u0d24\u0d30 \u0d35\u0d3f\u0d28\u0d41\u0d24\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d15\u0d2e\u0d32\u0d3e\u0d2a\u0d41\u0d30 \u0d38\u0d26\u0d28\u0d47 \u0d15\u0d2e\u0d28\u0d40\u0d2f \u0d35\u0d26\u0d28\u0d47 \u0d15\u0d30\u0d41\u0d23\u0d3e\u0d2f\u0d24 \u0d28\u0d2f\u0d28\u0d47 \u0d15\u0d30 <br/> \u0d15\u0d02\u0d1c\u0d28\u0d47\u0d15\u0d4d\u0d37\u0d41 \u0d27\u0d28\u0d41\u0d30\u0d3e\u0d15\u0d4d\u0d30\u0d4d\u0d24 \u0d17\u0d2e\u0d4d\u0d2d\u0d40\u0d30 \u0d2e\u0d27\u0d41\u0d15\u0d48\u0d1f\u0d2d \u0d38\u0d26\u0d28\u0d47 <br/> \u0d17\u0d02\u0d17\u0d3e \u0d2d\u0d41\u0d1c\u0d02\u0d17 \u0d15\u0d2e\u0d32 \u0d35\u0d48\u0d30\u0d3f\u0d24\u0d30 \u0d1c\u0d1f\u0d3e \u0d2e\u0d15\u0d41\u0d1f \u0d15\u0d32\u0d4d\u0d32\u0d4b\u0d32 \u0d35\u0d3f\u0d32\u0d4b\u0d15\u0d4d\u0d2f <br/> \u0d15\u0d30\u0d3e\u0d27\u0d41\u0d30\u0d41\u0d15\u0d4d\u0d30\u0d2e \u0d32\u0d40\u0d32\u0d47 \u0d28\u0d3f\u0d26\u0d41\u0d35\u0d28 \u0d36\u0d40\u0d32\u0d47 \u0d38 \u0d28\u0d3f\u0d17\u0d2e \u0d32\u0d40\u0d32\u0d47 \u0d1c\u0d2f \u0d1c\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d32\u0d32\u0d3f\u0d24\u0d47 \u0d32\u0d3e\u0d35\u0d23\u0d4d\u0d2f \u0d38\u0d3e\u0d30\u0d2e\u0d02\u0d17\u0d33 \u0d35\u0d3f\u0d17\u0d4d\u0d30\u0d39\u0d47 \u0d15\u0d32\u0d4d\u0d2a\u0d32\u0d24\u0d3f\u0d15\u0d47 \u0d32\u0d15\u0d3e\u0d30 <br/> \u0d32\u0d3e\u0d32\u0d28 \u0d24\u0d24\u0d4d\u0d35 \u0d28\u0d3f\u0d30\u0d3e\u0d2e\u0d2f\u0d3f \u0d1a\u0d3f\u0d28\u0d4d\u0d2e\u0d2f\u0d3f \u0d32\u0d2e\u0d4d\u0d2c\u0d4b\u0d27\u0d30 \u0d17\u0d41\u0d30\u0d41\u0d17\u0d41\u0d39 \u0d1c\u0d28\u0d28\u0d3f <br/> \u0d32\u0d32\u0d3e\u0d2e \u0d2e\u0d30\u0d15\u0d24 \u0d32\u0d3e\u0d32\u0d2f\u0d24 \u0d32\u0d32\u0d3e\u0d1f \u0d38\u0d3f\u0d28\u0d4d\u0d27\u0d42\u0d30 \u0d24\u0d3f\u0d32\u0d15\u0d47 \u0d32\u0d17\u0d41\u0d27\u0d30 <br/> \u0d38\u0d30\u0d4d\u0d35\u0d3e\u0d30\u0d4d\u0d17\u0d2a\u0d35\u0d30\u0d4d\u0d17 \u0d38\u0d41\u0d16\u0d26\u0d3e\u0d2f\u0d3f\u0d28\u0d47 \u0d38\u0d41\u0d2e\u0d24\u0d47 \u0d2e\u0d39\u0d3e\u0d26\u0d47\u0d35\u0d3f \u0d38\u0d30\u0d38\u0d3f\u0d1c\u0d3e\u0d15\u0d4d\u0d37\u0d3f"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u094d\u0930\u0940 \u091a\u0915\u094d\u0930 \u092e\u093e\u0924\u0902\u0917\u093f\u0928\u0940 \u0926\u0947\u0935\u0926\u0947\u0935 \u0936\u0948\u0932\u0947\u0928\u094d\u0926\u094d\u0930 \u0928\u0928\u094d\u0926\u093f\u0928\u093f \u0936\u093f\u0935\u092d\u093e\u0917\u093e \u0928\u0928\u094d\u0926\u093f\u0928\u0940\u0915\u093e\u0928\u094d\u0924 <br/> \u0928\u093f\u0930\u0928\u094d\u0924\u0930 \u0915\u0926\u092e\u094d\u092c\u0935\u0928 \u0930\u0902\u091c\u0928\u093f \u0915\u093e\u092e\u0915\u094b\u091f\u093f \u0915\u0932\u093e\u092a\u0940\u0920 \u0935\u093f\u0930\u093e\u091c\u093f\u0928\u093f"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092d\u0942 \u091a\u0915\u094d\u0930 \u092e\u0947\u0930\u0941 \u0928\u093f\u0915\u0947\u0924\u0928\u0947 \u092d\u0941\u0935\u0928\u0947\u0936\u094d\u0935\u0930\u093f \u0939\u092e\u094d\u0938\u0917\u092e\u0928\u0947 \u092a\u0941\u0930\u