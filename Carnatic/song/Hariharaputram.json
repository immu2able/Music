u0c41\u0c24\u0c4d\u0c30\u0c02 \u0c36\u0c3e\u0c38\u0c4d\u0c24\u0c3e\u0c30\u0c2e\u0c4d \u0c38\u0c26\u0c3e \u0c2d\u0c1c\u0c47\u0c39\u0c2e\u0c4d \u0c2e\u0c3e\u0c2f\u0c3e \u0c15\u0c3e\u0c30\u0c4d\u0c2f\u0c2e\u0c4d \u0c24\u0c4d\u0c2f\u0c1c\u0c47\u0c39\u0c2e\u0c4d"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c41\u0c30 \u0c39\u0c30\u0c3e\u0c26\u0c3f \u0c2e\u0c4b\u0c39\u0c3f\u0c24 \u0c36\u0c4c\u0c30\u0c3f \u0c17\u0c3f\u0c30\u0c3f \u0c35\u0c3f\u0c39\u0c3e\u0c30\u0c2e\u0c4d \u0c2e\u0c41\u0c30\u0c33\u0c40 \u0c2d\u0c47\u0c30\u0c40 \u0c35\u0c3e\u0c26\u0c4d\u0c2f\u0c3e\u0c26\u0c3f \u0c2a\u0c4d\u0c30\u0c3f\u0c2f\u0c15\u0c30\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c2a\u0c4d\u0c30\u0c3e\u0c30\u0c4d\u0c25\u0c3f\u0c24 \u0c2a\u0c41\u0c24\u0c4d\u0c30 \u0c2a\u0c4d\u0c30\u0c26\u0c2e\u0c4d \u0c35\u0c38\u0c28\u0c4d\u0c24 \u0c28\u0c24 \u0c2c\u0c43\u0c28\u0c4d\u0c26\u0c2e\u0c4d \u0c26\u0c40\u0c30\u0c4d\u0c18\u0c3e\u0c2f\u0c41\u0c37\u0c4d\u0c2a\u0c4d\u0c30\u0c26 \u0c26\u0c40\u0c28 \u0c1c\u0c28 \u0c38\u0c41\u0c16\u0c2a\u0c4d\u0c30\u0c26\u0c2e\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2b\u0c3e\u0c32\u0c4d\u0c17\u0c41\u0c28 \u0c2e\u0c3e\u0c38 \u0c2a\u0c4c\u0c30\u0c4d\u0c23\u0c3f\u0c2e\u0c3e\u0c35\u0c24\u0c3e\u0c30\u0c2e\u0c4d \u0c2a\u0c3e\u0c23\u0c4d\u0c21\u0c4d\u0c2f \u0c15\u0c47\u0c30\u0c33\u0c3e\u0c26\u0c3f \u0c26\u0c47\u0c36 \u0c2a\u0c4d\u0c30\u0c2d\u0c3e\u0c15\u0c30\u0c2e\u0c4d <br/> \u0c2a\u0c41\u0c37\u0c4d\u0c2a \u0c36\u0c30\u0c47\u0c15\u0c4d\u0c37\u0c41 \u0c15\u0c3e\u0c30\u0c4d\u0c2e\u0c41\u0c15 \u0c27\u0c30\u0c2e\u0c4d \u0c2b\u0c41\u0c32\u0c4d\u0c32 \u0c15\u0c32\u0c4d\u0c39\u0c3e\u0c30 \u0c26\u0c23\u0c4d\u0c21\u0c27\u0c30\u0c15\u0c30\u0c2e\u0c4d"}, {"M": "\u0c2e\u0c27\u0c4d\u0c2f\u0c2e\u0c15\u0c3e\u0c32 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c32\u0c3f\u0c2f\u0c41\u0c17 \u0c2a\u0c4d\u0c30\u0c24\u0c4d\u0c2f\u0