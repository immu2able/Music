u0bcd\u0b9f\u0bc1\u0b95\u0bcd \u0b95\u0ba9\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1 \u0bb5\u0bb0\u0bc1\u0bae\u0bcd \u0baa\u0b9a\u0bc1 \u0baa\u0bcb\u0bb2\u0bcd <br/> \u0b92\u0ba9\u0bcd\u0bb1\u0bc1\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd \u0b85\u0b9e\u0bcd\u0b9a\u0bbe\u0ba4 \u0b8e\u0ba9\u0bcd \u0b89\u0bb3\u0bcd\u0bb3\u0ba4\u0bcd\u0ba4\u0bc1\u0baf\u0bb0\u0bae\u0bcd \u0ba8\u0bc0\u0b95\u0bcd\u0b95"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b89\u0ba3\u0bcd\u0b9f\u0bbe\u0ba9 \u0baa\u0bcb\u0ba4\u0bc1 \u0b95\u0bcb\u0b9f\u0bbf \u0b89\u0bb1 \u0bae\u0bc1\u0bb1\u0bc8\u0baf\u0bcb\u0bb0\u0bcd\u0b95\u0bb3\u0bcd \u0bb5\u0ba8\u0bcd\u0ba4\u0bc1 <br/> \u0b95\u0bca\u0ba3\u0bcd\u0b9f\u0bbe\u0b9f\u0bbf\u0ba4\u0bcd \u0ba4\u0bca\u0ba3\u0bcd\u0b9f\u0bbe\u0b9f\u0bbf\u0b95\u0bcd \u0b95\u0bca\u0bb3\u0bcd\u0bb5\u0bbe\u0bb0\u0bcd-- \u0ba4\u0ba9\u0bae\u0bcd \u0b95\u0bc1\u0bb1\u0bc8\u0ba8\u0bcd\u0ba4\u0bbe\u0bb2\u0bcd <br/> \u0b95\u0ba3\u0bcd\u0b9f\u0bbe\u0bb2\u0bc1\u0bae\u0bcd \u0baa\u0bc7\u0b9a\u0bbe\u0bb0\u0bcd \u0b87\u0ba8\u0bcd\u0ba4 \u0b95\u0bc8\u0ba4\u0bcd\u0ba4\u0bb5\u0bae\u0bbe\u0ba9 \u0baa\u0bca\u0bb2\u0bcd\u0bb2\u0bbe\u0b9a\u0bcd <br/> \u0b9a\u0ba3\u0bcd\u0b9f\u0bbe\u0bb3 \u0b89\u0bb2\u0b95\u0ba4\u0bcd\u0ba4\u0bc8\u0ba4\u0bcd \u0ba4\u0bb3\u0bcd\u0bb3\u0bbf \u0ba8\u0bb1\u0bcd\u0b95\u0ba4\u0bbf \u0b9a\u0bc6\u0bb2\u0bcd\u0bb2"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0ba8\u0bbe\u0ba9\u0bcd  \u0b95\u0bca\u0ba3\u0bcd\u0b9f  \u0ba4\u0bc1\u0baf\u0bb0\u0ba4\u0bcd\u0ba4\u0bc8 \u0ba8\u0bbe\u0ba9\u0bcd \u0bb5\u0bc6\u0ba9\u0bcd\u0bb1\u0bc7\u0ba9\u0bcd \u0b8e\u0ba9\u0b95\u0bcd\u0b95\u0bbe\u0b9f\u0bcd\u0b9f\u0bbf <br/> \u0bb5\u0bc0\u0ba3\u0bcd \u0b95\u0ba3\u0bcd\u0b9f \u0b87\u0bb0\u0bc1\u0bb3\u0bc6\u0bb2\u0bcd\u0bb2\u0bbe\u0bae\u0bcd \u0bb5\u0bc6\u0bb3\u0bbf\u0baf\u0bbe\u0b95\u0bcd\u0b95\u0bbf\u0baa\u0bcd  \u0baa\u0bca\u0bb0\u0bc1\u0bb3\u0bcd \u0ba8\u0bbe\u0b9f\u0bcd\u0b9f\u0bbf <br/> \u0baf\u0bbe\u0ba9\u0bcd \u0b87\u0ba9\u0bcd\u0bb1\u0bc1\u0bb3\u0bcd \u0b87\u0bb3\u0bc8\u0baa\u0bcd\u0baa\u0bbe\u0bb1\u0bbf \u0b86\u0ba9\u0ba8\u0bcd\u0ba4\u0baa\u0bcd \u0baa\u0bc6\u0bb0\u0bc1\u0ba8\u0bcd\u0ba4\u0bc0\u0bb0\u0ba9\u0bcd <br/> \u0ba4\u0bbe\u0ba9\u0bcd \u0ba8\u0bbf\u0ba9\u0bcd\u0bb1 \u0ba8\u0bc0\u0bb2\u0b95\u0ba3\u0bcd\u0b9f\u0ba9\u0bcd \u0b9a\u0bb0\u0ba3\u0bbe\u0bb0\u0bb5\u0bbf\u0ba8\u0bcd\u0ba4\u0bae\u0bcd \u0b9a\u0bc7\u0bb0"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c0e\u0c28\u0c4d\u0c31\u0c48\u0c15\u0c4d\u0c15\u0c41 \u0c38\u0c3f\u0c35 \u0c15\u0c4d\u0c30\u0c41\u0c2a\u0c48 \u0c35\u0c30\u0c41\u0c2e\u0c4b, \u0c0f\u0c34\u0c48 <br/> \u0c0e\u0c28\u0c4d \u0c2e\u0c28 \u0c1a\u0c1e\u0c4d\u0c1a\u0c32\u0c2e\u0c4d \u0c05\u0c31\u0c41\u0c2e\u0c4b"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c15\u0c28\u0c4d\u0c31\u0c3f\u0c28\u0c4d \u0c15\u0c41\u0c30\u0c32\u0c48\u0c15\u0c4d \u0c15\u0c47\u0c1f\u0c4d\u0c1f\u0c41\u0c15\u0c4d \u0c15\u0c28\u0c3f\u0c28\u0c4d\u0c24\u0c41 \u0c35\u0c30\u0c41\u0c2e\u0c4d \u0c2a\u0c1a\u0c41 \u0c2a\u0c4b\u0c32\u0c4d <br/> \u0c12\u0c28\u0c4d\u0c31\u0c41\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d \u0c05\u0c1e\u0c4d\u0c1a\u0c3e\u0c24 \u0c0e\u0c28\u0c4d \u0c09\u0c33\u0c4d\u0c33\u0c24\u0c4d\u0c24\u0c41\u0c2f\u0c30\u0c2e\u0c4d \u0c28\u0c40\u0c15\u0c4d\u0c15"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c09\u0c23\u0c4d\u0c1f\u0c3e\u0c28 \u0c2a\u0c4b\u0c24\u0c41 \u0c15\u0c4b\u0c1f\u0c3f \u0c09\u0c31 \u0c2e\u0c41\u0c31\u0c48\u0c2f\u0c4b\u0c30\u0c4d\u0c15\u0c33\u0c4d \u0c35\u0c28\u0c4d\u0c24\u0c41 <br/> \u0c15\u0c4a\u0c23\u0c4d\u0c1f\u0c3e\u0c1f\u0c3f\u0c24\u0c4d \u0c24\u0c4a\u0c23\u0c4d\u0c1f\u0c3e\u0c1f\u0c3f\u0c15\u0c4d \u0c15\u0c4a\u0c33\u0c4d\u0c35\u0c3e\u0c30\u0c4d-- \u0c24\u0c28\u0c2e\u0c4d \u0c15\u0c41\u0c31\u0c48\u0c28\u0c4d\u0c24\u0c3e\u0c32\u0c4d <br/> \u0c15\u0c23\u0c4d\u0c1f\u0c3e\u0c32\u0c41\u0c2e\u0c4d \u0c2a\u0c47\u0c1a\u0c3e\u0c30\u0c4d \u0c07\u0c28\u0c4d\u0c24 \u0c15\u0c48\u0c24\u0c4d\u0c24\u0c35\u0c2e\u0c3e\u0c28 \u0c2a\u0c4a\u0c32\u0c4d\u0c32\u0c3e\u0c1a\u0c4d <br/> \u0c1a\u0c23\u0c4d\u0c1f\u0c3e\u0c33 \u0c09\u0c32\u0c15\u0c24\u0c4d\u0c24\u0c48\u0c24\u0c4d \u0c24\u0c33\u0c4d\u0c33\u0c3f \u0c28\u0c31\u0c4d\u0c15\u0c24\u0c3f \u0c1a\u0c46\u0c32\u0c4d\u0c32"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c28\u0c3e\u0c28\u0c4d  \u0c15\u0c4a\u0c23\u0c4d\u0c1f  \u0c24\u0c41\u0c2f\u0c30\u0c24\u0c4d\u0c24\u0c48 \u0c28\u0c3e\u0c28\u0c4d \u0c35\u0c46\u0c28\u0c4d\u0c31\u0c47\u0c28\u0c4d \u0c0e\u0c28\u0c15\u0c4d\u0c15\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f <br/> \u0c35\u0c40\u0c23\u0c4d \u0c15\u0c23\u0c4d\u0c1f \u0c07\u0c30\u0c41\u0c33\u0c46\u0c32\u0c4d\u0c32\u0c3e\u0c2e\u0c4d \u0c35\u0c46\u0c33\u0c3f\u0c2f\u0c3e\u0c15\u0c4d\u0c15\u0c3f\u0c2a\u0c4d  \u0c2a\u0c4a\u0c30\u0c41\u0c33\u0c4d \u0c28\u0c3e\u0c1f\u0c4d\u0c1f\u0c3f <br/> \u0c2f\u0c3e\u0c28\u0c4d \u0c07\u0c28\u0c4d\u0c31\u0c41\u0c33\u0c4d \u0c07\u0c33\u0c48\u0c2a\u0c4d\u0c2a\u0c3e\u0c31\u0c3f \u0c06\u0c28\u0c28\u0c4d\u0c24\u0c2a\u0c4d \u0c2a\u0c46\u0c30\u0c41\u0c28\u0c4d\u0c24\u0c40\u0c30\u0c28\u0c4d <br/> \u0c24\u0c3e\u0c28\u0c4d \u0c28\u0c3f\u0c28\u0c4d\u0c31 \u0c28\u0c40\u0c32\u0c15\u0c23\u0c4d\u0c1f\u0c28\u0c4d \u0c1a\u0c30\u0c23\u0c3e\u0c30\u0c35\u0c3f\u0c28\u0c4d\u0c24\u0c2e\u0c4d \u0c1a\u0c47\u0c30"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c8e\u0ca8\u0ccd\u0cb1\u0cc8\u0c95\u0ccd\u0c95\u0cc1 \u0cb8\u0cbf\u0cb5 \u0c95\u0ccd\u0cb0\u0cc1\u0caa\u0cc8 \u0cb5\u0cb0\u0cc1\u0cae\u0ccb, \u0c8f\u0cb3\u0cc8 <br/> \u0c8e\u0ca8\u0ccd \u0cae\u0ca8 \u0c9a\u0c9e\u0ccd\u0c9a\u0cb2\u0cae\u0ccd \u0c85\u0cb1\u0cc1\u0cae\u0ccb"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0ca8\u0ccd\u0cb1\u0cbf\u0ca8\u0ccd \u0c95\u0cc1\u0cb0\u0cb2\u0cc8\u0c95\u0ccd \u0c95\u0cc7\u0c9f\u0ccd\u0c9f\u0cc1\u0c95\u0ccd \u0c95\u0ca8\u0cbf\u0ca8\u0ccd\u0ca4\u0cc1 \u0cb5\u0cb0\u0cc1\u0cae\u0ccd \u0caa\u0c9a\u0cc1 \u0caa\u0ccb\u0cb2\u0ccd <br/> \u0c92\u0ca8\u0ccd\u0cb1\u0cc1\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c85\u0c9e\u0ccd\u0c9a\u0cbe\u0ca4 \u0c8e\u0ca8\u0ccd \u0c89\u0cb3\u0ccd\u0cb3\u0ca4\u0ccd\u0ca4\u0cc1\u0caf\u0cb0\u0cae\u0ccd \u0ca8\u0cc0\u0c95\u0ccd\u0c95"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c89\u0ca3\u0ccd\u0c9f\u0cbe\u0ca8 \u0caa\u0ccb\u0ca4\u0cc1 \u0c95\u0ccb\u0c9f\u0cbf \u0c89\u0cb1 \u0cae\u0cc1\u0cb1\u0cc8\u0caf\u0ccb\u0cb0\u0ccd\u0c95\u0cb3\u0ccd \u0cb5\u0ca8\u0ccd\u0ca4\u0cc1 <br/> \u0c95\u0cca\u0ca3\u0ccd\u0c9f\u0cbe\u0c9f\u0cbf\u0ca4\u0ccd \u0ca4\u0cca\u0ca3\u0ccd\u0c9f\u0cbe\u0c9f\u0cbf\u0c95\u0ccd \u0c95\u0cca\u0cb3\u0ccd\u0cb5\u0cbe\u0cb0\u0ccd-- \u0ca4\u0ca8\u0cae\u0ccd \u0c95\u0cc1\u0cb1\u0cc8\u0ca8\u0ccd\u0ca4\u0cbe\u0cb2\u0ccd <br/> \u0c95\u0ca3\u0ccd\u0c9f\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0caa\u0cc7\u0c9a\u0cbe\u0cb0\u0ccd \u0c87\u0ca8\u0ccd\u0ca4 \u0c95\u0cc8\u0ca4\u0ccd\u0ca4\u0cb5\u0cae\u0cbe\u0ca8 \u0caa\u0cca\u0cb2\u0ccd\u0cb2\u0cbe\u0c9a\u0ccd <br/> \u0c9a\u0ca3\u0ccd\u0c9f\u0cbe\u0cb3 \u0c89\u0cb2\u0c95\u0ca4\u0ccd\u0ca4\u0cc8\u0ca4\u0ccd \u0ca4\u0cb3\u0ccd\u0cb3\u0cbf \u0ca8\u0cb1\u0ccd\u0c95\u0ca4\u0cbf \u0c9a\u0cc6\u0cb2\u0ccd\u0cb2"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0ca8\u0cbe\u0ca8\u0ccd  \u0c95\u0cca\u0ca3\u0ccd\u0c9f  \u0ca4\u0cc1\u0caf\u0cb0\u0ca4\u0ccd\u0ca4\u0cc8 \u0ca8\u0cbe\u0ca8\u0ccd \u0cb5\u0cc6\u0ca8\u0ccd\u0cb1\u0cc7\u0ca8\u0ccd \u0c8e\u0ca8\u0c95\u0ccd\u0c95\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf <br/> \u0cb5\u0cc0\u0ca3\u0ccd \u0c95\u0ca3\u0ccd\u0c9f \u0c87\u0cb0\u0cc1\u0cb3\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0cb5\u0cc6\u0cb3\u0cbf\u0caf\u0cbe\u0c95\u0ccd\u0c95\u0cbf\u0caa\u0ccd  \u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0ccd \u0ca8\u0cbe\u0c9f\u0ccd\u0c9f\u0cbf <br/> \u0caf\u0cbe\u0ca8\u0ccd \u0c87\u0ca8\u0ccd\u0cb1\u0cc1\u0cb3\u0ccd \u0c87\u0cb3\u0cc8\u0caa\u0ccd\u0caa\u0cbe\u0cb1\u0cbf \u0c86\u0ca8\u0ca8\u0ccd\u0ca4\u0caa\u0ccd \u0caa\u0cc6\u0cb0\u0cc1\u0ca8\u0ccd\u0ca4\u0cc0\u0cb0\u0ca8\u0ccd <br/> \u0ca4\u0cbe\u0ca8\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1 \u0ca8\u0cc0\u0cb2\u0c95\u0ca3\u0ccd\u0c9f\u0ca8\u0ccd \u0c9a\u0cb0\u0ca3\u0cbe\u0cb0\u0cb5\u0cbf\u0ca8\u0ccd\u0ca4\u0cae\u0ccd \u0c9a\u0cc7\u0cb0"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d0e\u0d28\u0d4d\u0d31\u0d48\u0d15\u0d4d\u0d15\u0d41 \u0d38\u0d3f\u0d35 \u0d15\u0d4d\u0d30\u0d41\u0d2a\u0d48 \u0d35\u0d30\u0d41\u0d2e\u0d4b, \u0d0f\u0d34\u0d48 <br/> \u0d0e\u0d28\u0d4d \u0d2e\u0d28 \u0d1a\u0d1e\u0d4d\u0d1a\u0d32\u0d2e\u0d4d \u0d05\u0d31\u0d41\u0d2e\u0d4b"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d28\u0d4d\u0d31\u0d3f\u0d28\u0d4d \u0d15\u0d41\u0d30\u0d32\u0d48\u0d15\u0d4d \u0d15\u0d47\u0d1f\u0d4d\u0d1f\u0d41\u0d15\u0d4d \u0d15\u0d28\u0d3f\u0d28\u0d4d\u0d24\u0d41 \u0d35\u0d30\u0d41\u0d2e\u0d4d \u0d2a\u0d1a\u0d41 \u0d2a\u0d4b\u0d32\u0d4d <br/> \u0d12\u0d28\u0d4d\u0d31\u0d41\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d05\u0d1e\u0d4d\u0d1a\u0d3e\u0d24 \u0d0e\u0d28\u0d4d \u0d09\u0d33\u0d4d\u0d33\u0d24\u0d4d\u0d24\u0d41\u0d2f\u0d30\u0d2e\u0d4d \u0d28\u0d40\u0d15\u0d4d\u0d15"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d09\u0d23\u0d4d\u0d1f\u0d3e\u0d28 \u0d2a\u0d4b\u0d24\u0d41 \u0d15\u0d4b\u0d1f\u0d3f \u0d09\u0d31 \u0d2e\u0d41\u0d31\u0d48\u0d2f\u0d4b\u0d30\u0d4d\u0d15\u0d33\u0d4d \u0d35\u0d28\u0d4d\u0d24\u0d41 <br/> \u0d15\u0d4a\u0d23\u0d4d\u0d1f\u0d3e\u0d1f\u0d3f\u0d24\u0d4d \u0d24\u0d4a\u0d23\u0d4d\u0d1f\u0d3e\u0d1f\u0d3f\u0d15\u0d4d \u0d15\u0d4a\u0d33\u0d4d\u0d35\u0d3e\u0d30\u0d4d-- \u0d24\u0d28\u0d2e\u0d4d \u0d15\u0d41\u0d31\u0d48\u0d28\u0d4d\u0d24\u0d3e\u0d32\u0d4d <br/> \u0d15\u0d23\u0d4d\u0d1f\u0d3e\u0d32\u0d41\u0d2e\u0d4d \u0d2a\u0d47\u0d1a\u0d3e\u0d30\u0d4d \u0d07\u0d28\u0d4d\u0d24 \u0d15\u0d48\u0d24\u0d4d\u0d24\u0d35\u0d2e\u0d3e\u0d28 \u0d2a\u0d4a\u0d32\u0d4d\u0d32\u0d3e\u0d1a\u0d4d <br/> \u0d1a\u0d23\u0d4d\u0d1f\u0d3e\u0d33 \u0d09\u0d32\u0d15\u0d24\u0d4d\u0d24\u0d48\u0d24\u0d4d \u0d24\u0d33\u0d4d\u0d33\u0d3f \u0d28\u0d31\u0d4d\u0d15\u0d24\u0d3f \u0d1a\u0d46\u0d32\u0d4d\u0d32"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d28\u0d3e\u0d28\u0d4d  \u0d1