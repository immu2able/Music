\u0c3f\u0c35\u0c47 \u0c32\u0c41\u0c21\u0c28\u0c47 \u0c35\u0c30\u0c41\u0c35\u0c3e\u0c2f\u0c4d <br/> \u0c24\u0c30\u0c41\u0c35\u0c3e\u0c2f\u0c4d \u0c28\u0c32\u0c2e\u0c41\u0c2e\u0c4d \u0c24\u0c17\u0c35\u0c41\u0c2e\u0c4d \u0c2a\u0c41\u0c17\u0c34\u0c41\u0c2e\u0c4d <br/> \u0c24\u0c35\u0c2e\u0c41\u0c2e\u0c4d \u0c24\u0c3f\u0c31\u0c2e\u0c41\u0c2e\u0c4d \u0c24\u0c28\u0c2e\u0c41\u0c2e\u0c4d \u0c15\u0c28\u0c2e\u0c41\u0c2e\u0c4d."}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c05\u0c21\u0c3f\u0c2f\u0c3e\u0c30\u0c4d \u0c2a\u0c32\u0c30\u0c3f\u0c19\u0c4d \u0c15\u0c41\u0c33\u0c30\u0c47 <br/> \u0c05\u0c35\u0c30\u0c48 \u0c35\u0c3f\u0c21\u0c41\u0c35\u0c3f\u0c24\u0c4d \u0c24\u0c30\u0c41\u0c33\u0c4d\u0c35\u0c3e\u0c2f\u0c4d <br/> \u0c2e\u0c41\u0c21\u0c3f\u0c2f\u0c3e \u0c2e\u0c31\u0c48\u0c2f\u0c3f\u0c28\u0c4d \u0c2e\u0c41\u0c21\u0c3f\u0c35\u0c47! \u0c05\u0c38\u0c41\u0c30\u0c30\u0c4d <br/> \u0c2e\u0c41\u0c21\u0c3f\u0c35\u0c47 \u0c15\u0c30\u0c41\u0c26\u0c41\u0c2e\u0c4d \u0c35\u0c21\u0c3f\u0c35\u0c47 \u0c32\u0c35\u0c28\u0c47!"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c38\u0c41\u0c30\u0c41\u0c26\u0c3f\u0c2a\u0c4d \u0c2a\u0c4a\u0c30\u0c41\u0c33\u0c47, \u0c35\u0c30\u0c41\u0c17. <br/> \u0c24\u0c41\u0c23\u0c3f\u0c35\u0c47, \u0c15\u0c28\u0c32\u0c47, \u0c35\u0c30\u0c41\u0c17. <br/> \u0c15\u0c30\u0c41\u0c26\u0c3f\u0c15\u0c4d \u0c15\u0c30\u0c41\u0c26\u0c3f\u0c15\u0c4d \u0c15\u0c35\u0c32\u0c48\u0c2a\u0c4d \u0c2a\u0c21\u0c41\u0c35\u0c3e\u0c30\u0c4d <br/> \u0c15\u0c35\u0c32\u0c48\u0c15\u0c4d \u0c15\u0c21\u0c32\u0c48\u0c15\u0c4d \u0c15\u0c21\u0c3f\u0c2f\u0c41\u0c2e\u0c4d \u0c35\u0c21\u0c3f\u0c35\u0c47\u0c32\u0c4d"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c05\u0c2e\u0c30\u0c3e \u0c35\u0c26\u0c3f\u0c35\u0c3e\u0c34\u0c4d \u0c35\u0c41\u0c31\u0c35\u0c47 <br/> \u0c05\u0c30\u0c41\u0c33\u0c4d\u0c35\u0c3e\u0c2f\u0c4d! \u0c1a\u0c30\u0c23\u0c2e\u0c4d, \u0c1a\u0c30\u0c23\u0c2e\u0c4d! <br/> \u0c15\u0c41\u0c2e\u0c30\u0c3e, \u0c2a\u0c3f\u0c23\u0c3f\u0c2f\u0c3e \u0c35\u0c48\u0c2f\u0c41\u0c2e\u0c47 \u0c38\u0c3f\u0c26\u0c31\u0c15\u0c4d <br/> \u0c15\u0c41\u0c2e\u0c41\u0c31\u0c41\u0c2e\u0c4d \u0c38\u0c41\u0c21\u0c30\u0c4d\u0c35\u0c47 \u0c32\u0c35\u0c28\u0c47, \u0c1a\u0c30\u0c23\u0c2e\u0c4d."}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 5", "T": "\u0c05\u0c31\u0c3f\u0c35\u0c3e \u0c15\u0c3f\u0c2f\u0c17\u0c4b \u0c2f\u0c3f\u0c32\u0c3f\u0c32\u0c47 <br/> \u0c05\u0c30\u0c41\u0c33\u0c3e \u0c15\u0c3f\u0c2f\u0c26\u0c3e\u0c2f\u0c4d \u0c2e\u0c21\u0c3f\u0c2e\u0c47\u0c32\u0c4d <br/> \u0c2a\u0c4a\u0c31\u0c3f\u0c35\u0c47 \u0c32\u0c41\u0c21\u0c28\u0c47, \u0c35\u0c33\u0c30\u0c4d\u0c35\u0c3e\u0c2f\u0c4d, \u0c05\u0c21\u0c3f\u0c2f\u0c3e\u0c30\u0c4d <br/> \u0c2a\u0c41\u0c26\u0c41\u0c35\u0c3e\u0c34\u0c4d \u0c35\u0c41\u0c31\u0c35\u0c47 \u0c2a\u0c41\u0c35\u0c3f\u0c2e\u0c40 \u0c24\u0c30\u0c41\u0c33\u0c4d\u0c35\u0c3e\u0c2f\u0c4d."}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 6", "T": "\u0c15\u0c41\u0c30\u0c41\u0c35\u0c47, \u0c2a\u0c30\u0c2e\u0c28\u0c4d \u0c2e\u0c17\u0c28\u0c47, <br/> \u0c15\u0c41\u0c17\u0c48\u0c2f\u0c3f\u0c32\u0c4d \u0c35\u0c33\u0c30\u0c41\u0c19\u0c4d \u0c15\u0c28\u0c32\u0c47, <br/> \u0c24\u0c30\u0c41\u0c35\u0c3e\u0c2f\u0c4d \u0c24\u0c4a\u0c34\u0c3f\u0c32\u0c41\u0c2e\u0c4d \u0c2a\u0c2f\u0c28\u0c41\u0c2e\u0c4d, \u0c05\u0c2e\u0c30\u0c30\u0c4d <br/> \u0c38\u0c2e\u0c30\u0c3e \u0c24\u0c3f\u0c2c\u0c28\u0c47, \u0c1a\u0c30\u0c23\u0c2e\u0c4d, \u0c1a\u0c30\u0c23\u0c2e\u0c4d."}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cc1\u0cb0\u0cc1\u0c97\u0cbe \u0cae\u0cc1\u0cb0\u0cc1\u0c97\u0cbe \u0cae\u0cc1\u0cb0\u0cc1\u0c97\u0cbe"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb5\u0cb0\u0cc1\u0cb5\u0cbe\u0caf\u0ccd \u0cae\u0caf\u0cbf\u0cb2\u0ccd \u0cae\u0cc0\u0ca6\u0cbf\u0ca8\u0cbf\u0cb2\u0cc7 <br/> \u0cb5\u0ca1\u0cbf\u0cb5\u0cc7 \u0cb2\u0cc1\u0ca1\u0ca8\u0cc7 \u0cb5\u0cb0\u0cc1\u0cb5\u0cbe\u0caf\u0ccd <br/> \u0ca4\u0cb0\u0cc1\u0cb5\u0cbe\u0caf\u0ccd \u0ca8\u0cb2\u0cae\u0cc1\u0cae\u0ccd \u0ca4\u0c97\u0cb5\u0cc1\u0cae\u0ccd \u0caa\u0cc1\u0c97\u0cb3\u0cc1\u0cae\u0ccd <br/> \u0ca4\u0cb5\u0cae\u0cc1\u0cae\u0ccd \u0ca4\u0cbf\u0cb1\u0cae\u0cc1\u0cae\u0ccd \u0ca4\u0ca8\u0cae\u0cc1\u0cae\u0ccd \u0c95\u0ca8\u0cae\u0cc1\u0cae\u0ccd."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c85\u0ca1\u0cbf\u0caf\u0cbe\u0cb0\u0ccd \u0caa\u0cb2\u0cb0\u0cbf\u0c99\u0ccd \u0c95\u0cc1\u0cb3\u0cb0\u0cc7 <br/> \u0c85\u0cb5\u0cb0\u0cc8 \u0cb5\u0cbf\u0ca1\u0cc1\u0cb5\u0cbf\u0ca4\u0ccd \u0ca4\u0cb0\u0cc1\u0cb3\u0ccd\u0cb5\u0cbe\u0caf\u0ccd <br/> \u0cae\u0cc1\u0ca1\u0cbf\u0caf\u0cbe \u0cae\u0cb1\u0cc8\u0caf\u0cbf\u0ca8\u0ccd \u0cae\u0cc1\u0ca1\u0cbf\u0cb5\u0cc7! \u0c85\u0cb8\u0cc1\u0cb0\u0cb0\u0ccd <br/> \u0cae\u0cc1\u0ca1\u0cbf\u0cb5\u0cc7 \u0c95\u0cb0\u0cc1\u0ca6\u0cc1\u0cae\u0ccd \u0cb5\u0ca1\u0cbf\u0cb5\u0cc7 \u0cb2\u0cb5\u0ca8\u0cc7!"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb8\u0cc1\u0cb0\u0cc1\u0ca6\u0cbf\u0caa\u0ccd \u0caa\u0cca\u0cb0\u0cc1\u0cb3\u0cc7, \u0cb5\u0cb0\u0cc1\u0c97. <br/> \u0ca4\u0cc1\u0ca3\u0cbf\u0cb5\u0cc7, \u0c95\u0ca8\u0cb2\u0cc7, \u0cb5\u0cb0\u0cc1\u0c97. <br/> \u0c95\u0cb0\u0cc1\u0ca6\u0cbf\u0c95\u0ccd \u0c95\u0cb0\u0cc1\u0ca6\u0cbf\u0c95\u0ccd \u0c95\u0cb5\u0cb2\u0cc8\u0caa\u0ccd \u0caa\u0ca1\u0cc1\u0cb5\u0cbe\u0cb0\u0ccd <br/> \u0c95\u0cb5\u0cb2\u0cc8\u0c95\u0ccd \u0c95\u0ca1\u0cb2\u0cc8\u0c95\u0ccd \u0c95\u0ca1\u0cbf\u0caf\u0cc1\u0cae\u0ccd \u0cb5\u0ca1\u0cbf\u0cb5\u0cc7\u0cb2\u0ccd"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c85\u0cae\u0cb0\u0cbe \u0cb5\u0ca6\u0cbf\u0cb5\u0cbe\u0cb3\u0ccd \u0cb5\u0cc1\u0cb1\u0cb5\u0cc7 <br/> \u0c85\u0cb0\u0cc1\u0cb3\u0ccd\u0cb5\u0cbe\u0caf\u0ccd! \u0c9a\u0cb0\u0ca3\u0cae\u0ccd, \u0c9a\u0cb0\u0ca3\u0cae\u0ccd! <br/> \u0c95\u0cc1\u0cae\u0cb0\u0cbe, \u0caa\u0cbf\u0ca3\u0cbf\u0caf\u0cbe \u0cb5\u0cc8\u0caf\u0cc1\u0cae\u0cc7 \u0cb8\u0cbf\u0ca6\u0cb1\u0c95\u0ccd <br/> \u0c95\u0cc1\u0cae\u0cc1\u0cb1\u0cc1\u0cae\u0ccd \u0cb8\u0cc1\u0ca1\u0cb0\u0ccd\u0cb5\u0cc7 \u0cb2\u0cb5\u0ca8\u0cc7, \u0c9a\u0cb0\u0ca3\u0cae\u0ccd."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c85\u0cb1\u0cbf\u0cb5\u0cbe \u0c95\u0cbf\u0caf\u0c97\u0ccb \u0caf\u0cbf\u0cb2\u0cbf\u0cb2\u0cc7 <br/> \u0c85\u0cb0\u0cc1\u0cb3\u0cbe \u0c95\u0cbf\u0caf\u0ca6\u0cbe\u0caf\u0ccd \u0cae\u0ca1\u0cbf\u0cae\u0cc7\u0cb2\u0ccd <br/> \u0caa\u0cca\u0cb1\u0cbf\u0cb5\u0cc7 \u0cb2\u0cc1\u0ca1\u0ca8\u0cc7, \u0cb5\u0cb3\u0cb0\u0ccd\u0cb5\u0cbe\u0caf\u0ccd, \u0c85\u0ca1\u0cbf\u0caf\u0cbe\u0cb0\u0ccd <br/> \u0caa\u0cc1\u0ca6\u0cc1\u0cb5\u0cbe\u0cb3\u0ccd \u0cb5\u0cc1\u0cb1\u0cb5\u0cc7 \u0caa\u0cc1\u0cb5\u0cbf\u0cae\u0cc0 \u0ca4\u0cb0\u0cc1\u0cb3\u0ccd\u0cb5\u0cbe\u0caf\u0ccd."}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c95\u0cc1\u0cb0\u0cc1\u0cb5\u0cc7, \u0caa\u0cb0\u0cae\u0ca8\u0ccd \u0cae\u0c97\u0ca8\u0cc7, <br/> \u0c95\u0cc1\u0c97\u0cc8\u0caf\u0cbf\u0cb2\u0ccd \u0cb5\u0cb3\u0cb0\u0cc1\u0c99\u0ccd \u0c95\u0ca8\u0cb2\u0cc7, <br/> \u0ca4\u0cb0\u0cc1\u0cb5\u0cbe\u0caf\u0ccd \u0ca4\u0cca\u0cb3\u0cbf\u0cb2\u0cc1\u0cae\u0ccd \u0caa\u0caf\u0ca8\u0cc1\u0cae\u0ccd, \u0c85\u0cae\u0cb0\u0cb0\u0ccd <br/> \u0cb8\u0cae\u0cb0\u0cbe \u0ca4\u0cbf\u0cac\u0ca8\u0cc7, \u0c9a\u0cb0\u0ca3\u0cae\u0ccd, \u0c9a\u0cb0\u0ca3\u0cae\u0ccd."}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d41\u0d30\u0d41\u0d17\u0d3e \u0d2e\u0d41\u0d30\u0d41\u0d17\u0d3e \u0d2e\u0d41\u0d30\u0d41\u0d17\u0d3e"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d35\u0d30\u0d41\u0d35\u0d3e\u0d2f\u0d4d \u0d2e\u0d2f\u0d3f\u0d32\u0d4d \u0d2e\u0d40\u0d26\u0d3f\u0d28\u0d3f\u0d32\u0d47 <br/> \u0d35\u0d21\u0d3f\u0d35\u0d47 \u0d32\u0d41\u0d21\u0d28\u0d47 \u0d35\u0d30\u0d41\u0d35\u0d3e\u0d2f\u0d4d <br/> \u0d24\u0d30\u0d41\u0d35\u0d3e\u0d2f\u0d4d \u0d28\u0d32\u0d2e\u0d41\u0d2e\u0d4d \u0d24\u0d17\u0d35\u0d41\u0d2e\u0d4d \u0d2a\u0d41\u0d17\u0d34\u0d41\u0d2e\u0d4d <br/> \u0d24\u0d35\u0d2e\u0d41\u0d2e\u0d4d \u0d24\u0d3f\u0d31\u0d2e\u0d41\u0d2e\u0d4d \u0d24\u0d28\u0d2e\u0d41\u0d2e\u0d4d \u0d15\u0d28\u0d2e\u0d41\u0d2e\u0d4d."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d05\u0d21\u0d3f\u0d2f\u0d3e\u0d30\u0d4d \u0d2a\u0d32\u0d30\u0d3f\u0d19\u0d4d \u0d15\u0d41\u0d33\u0d30\u0d47 <br/> \u0d05\u0d35\u0d30\u0d48 \u0d35\u0d3f\u0d21\u0d41\u0d35\u0d3f\u0d24\u0d4d \u0d24\u0d30\u0d41\u0d33\u0d4d\u0d35\u0d3e\u0d2f\u0d4d <br/> \u0d2e\u0d41\u0d21\u0d3f\u0d2f\u0d3e \u0d2e\u0d31\u0d48\u0d2f\u0d3f\u0d28\u0d4d \u0d2e\u0d41\u0d21\u0d3f\u0d35\u0d47! \u0d05\u0d38\u0d41\u0d30\u0d30\u0d4d <br/> \u0d2e\u0d41\u0d21\u0d3f\u0d35\u0d47 \u0d15\u0d30\u0d41\u0d26\u0d41\u0d2e\u0d4d \u0d35\u0d21\u0d3f\u0d35\u0d47 \u0d32\u0d35\u0d28\u0d47!"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d38\u0d41\u0d30\u0d41\u0d26\u0d3f\u0d2a\u0d4d \u0d2a\u0d4a\u0d30\u0d41\u0d33\u0d47, \u0d35\u0d30\u0d41\u0d17. <br/> \u0d24\u0d41\u0d23\u0d3f\u0d35\u0d47, \u0d15\u0d28\u0d32\u0d47, \u0d35\u0d30\u0d41\u0d17. <br/> \u0d15\u0d30\u0d41\u0d26\u0d3f\u0d15\u0d4d \u0d15\u0d30\u0d41\u0d26\u0d3f\u0d15\u0d4d \u0d15\u0d35\u0d32\u0d48\u0d2a\u0d4d \u0d2a\u0d21\u0d41\u0d35\u0d3e\u0d30\u0d4d <br/> \u0d15\u0d35\u0d32\u0d48\u0d15\u0d4d \u0d15\u0d21\u0d32\u0d48\u0d15\u0d4d \u0d15\u0d21\u0d3f\u0d2f\u0d41\u0d2e\u0d4d \u0d35\u0d21\u0d3f\u0d35\u0d47\u0d32\u0d4d"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d05\u0d2e\u0d30\u0d3e \u0d35\u0d26\u0d3f\u0d35\u0d3e\u0d34\u0d4d \u0d35\u0d41\u0d31\u0d35\u0d47 <br/> \u0d05\u0d30\u0d41\u0d33\u0d4d\u0d35\u0d3e\u0d2f\u0d4d! \u0d1a\u0d30\u0d23\u0d2e\u0d4d, \u0d1a\u0d30\u0d23\u0d2e\u0d4d! <br/> \u0d15\u0d41\u0d2e\u0d30\u0d3e, \u0d2a\u0d3f\u0d23\u0d3f\u0d2f\u0d3e \u0d35\u0d48\u0d2f\u0d41\u0d2e\u0d47 \u0d38\u0d3f\u0d26\u0d31\u0d15\u0d4d <br/> \u0d15\u0d41\u0d2e\u0d41\u0d31\u0d41\u0d2e\u0d4d \u0d38\u0d41\u0d21\u0d30\u0d4d\u0d35\u0d47 \u0d32\u0d35\u0d28\u0d47, \u0d1a\u0d30\u0d23\u0d2e\u0d4d."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d05\u0d31\u0d3f\u0d35\u0d3e \u0d15\u0d3f\u0d2f\u0d17\u0d4b \u0d2f\u0d3f\u0d32\u0d3f\u0d32\u0d47 <br/> \u0d05\u0d30\u0d41\u0d33\u0d3e \u0d15\u0d3f\u0d2f\u0d26\u0d3e\u0d2f\u0d4d \u0d2e\u0d21\u0d3f\u0d2e\u0d47\u0d32\u0d4d <br/> \u0d2a\u0d4a\u0d31\u0d3f\u0d35\u0d47 \u0d32\u0d41\u0d21\u0d28\u0d47, \u0d35\u0d33\u0d30\u0d4d\u0d35\u0d3e\u0d2f\u0d4d, \u0d05\u0d21\u0d3f\u0d2f\u0d3e\u0d30\u0d4d <br/> \u0d2a\u0d41\u0d26\u0d41\u0d35\u0d3e\u0d34\u0d4d \u0d35\u0d41\u0d31\u0d35\u0d47 \u0d2a\u0d41\u0d35\u0d3f\u0d2e\u0d40 \u0d24\u0d30\u0d41\u0d33\u0d4d\u0d35\u0d3e\u0d2f\u0d4d."}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "\u0d15\u0d41\u0d30\u0d41\u0d35\u0d47, \u0d2a\u0d30\u0d2e\u0d28\u0d4d \u0d2e\u0d17\u0d28\u0d47, <br/> \u0d15\u0d41\u0d17\u0d48\u0d2f\u0d3f\u0d32\u0d4d \u0d35\u0d33\u0d30\u0d41\u0d19\u0d4d \u0d15\u0d28\u0d32\u0d47, <br/> \u0d24\u0d30\u0d41\u0d35\u0d3e\u0d2f\u0d4d \u0d24\u0d4a\u0d34\u0d3f\u0d32\u0d41\u0d2e\u0d4d \u0d2a\u0d2f\u0d28\u0d41\u0d2e\u0d4d, \u0d05\u0d2e\u0d30\u0d30\u0d4d <br/> \u0d38\u0d2e\u0d30\u0d3e \u0d24\u0d3f\u0d2c\u0d28\u0d47, \u0d1a\u0d30\u0d23\u0d2e\u0d4d, \u0d1a\u0d30\u0d23\u0d2e\u0d4d."}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u0941\u0930\u0941\u0917\u093e \u092e\u0941\u0930\u0941\u0917\u093e \u092e\u0941\u0930\u0941\u0917\u093e"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0935\u0930\u0941\u0935\u093e\u092f\u094d \u092e\u092f\u093f\u0932\u094d \u092e\u0940\u0926\u093f\u0928\u093f\u0932\u0947 <br/> \u0935\u0921\u093f\u0935\u0947 \u0932\u0941\u0921\u0928\u0947 \u0935\u0930\u0941\u0935\u093e\u092f\u094d <br/> \u0924\u0930\u0941\u0935\u093e\u092f\u094d \u0928\u0932\u092e\u0941\u092e\u094d \u0924\u0917\u0935\u0941\u092e\u094d \u092a\u0941\u0917\u0932\u0941\u092e\u094d <br/> \u0924\u0935\u092e\u0941\u092e\u094d \u0924\u093f\u0930\u092e\u0941\u092e\u094d \u0924\u0928\u092e\u0941\u092e\u094d \u0915\u0928\u092e\u0941\u092e\u094d."}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0905\u0921\u093f\u092f\u093e\u0930\u094d \u092a\u0932\u0930\u093f\u0919\u094d \u0915\u0941\u0932\u0930\u0947 <br/> \u0905\u0935\u0930\u0948 \u0935\u093f\u0921\u0941\u0935\u093f\u0924\u094d \u0924\u0930\u0941\u0932\u094d\u0935\u093e\u092f\u094d <br/> \u092e\u0941\u0921\u093f\u092f\u093e \u092e\u0930\u0948\u092f\u093f\u0928\u094d \u092e\u0941\u0921\u093f\u0935\u0947! \u0905\u0938\u0941\u0930\u0930\u094d <br/> \u092e\u0941\u0921\u093f\u0935\u0947 \u0915\u0930\u0941\u0926\u0941\u092e\u094d \u0935\u0921\u093f\u0935\u0947 \u0932\u0935\u0928\u0947!"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0938\u0941\u0930\u0941\u0926\u093f\u092a\u094d \u092a\u094b\u0930\u0941\u0932\u0947, \u0935\u0930\u0941\u0917. <br/> \u0924\u0941\u0923\u093f\u0935\u0947, \u0915\u0928\u0932\u0947, \u0935\u0930\u0941\u0917. <br/> \u0915\u0930\u0941\u0926\u093f\u0915\u094d \u0915\u0930\u0941\u0926\u093f\u0915\u094d \u0915\u0935\u0932\u0948\u092a\u094d \u092a\u0921\u0941\u0935\u093e\u0930\u094d <br/> \u0915\u0935\u0932\u0948\u0915\u094d \u0915\u0921\u0932\u0948\u0915\u094d \u0915\u0921\u093f\u092f\u0941\u092e\u094d \u0935\u0921\u093f\u0935\u0947\u0932\u094d"}, {"M": "\u091a\u0930\u0923\u092e\u094d 4", "T": "\u0905\u092e\u0930\u093e \u0935\u0926\u093f\u0935\u093e\u0932\u094d \u0935\u0941\u0930\u0935\u0947 <br/> \u0905\u0930\u0941\u0932\u094d\u0935\u093e\u092f\u094d! \u091a\u0930\u0923\u092e\u094d, \u091a\u0930\u0923\u092e\u094d! <br/> \u0915\u0941\u092e\u0930\u093e, \u092a\u093f\u0923\u093f\u092f\u093e \u0935\u0948\u092f\u0941\u092e\u0947 \u0938\u093f\u0926\u0930\u0915\u094d <br/> \u0915\u0941\u092e\u0941\u0930\u0941\u092e\u094d \u0938\u0941\u0921\u0930\u094d\u0935\u0947 \u0932\u0935\u0928\u0947, \u091a\u0930\u0923\u092e\u094d."}, {"M": "\u091a\u0930\u0923\u092e\u094d 5", "T": "\u0905\u0930\u093f\u0935\u093e \u0915\u093f\u092f\u0917\u094b \u092f\u093f\u0932\u093f\u0932\u0947 <br/> \u0905\u0930\u0941\u0932\u093e \u0915\u093f\u092f\u0926\u093e\u092f\u094d \u092e\u0921\u093f\u092e\u0947\u0932\u094d <br/> \u092a\u094b\u0930\u093f\u0935\u0947 \u0932\u0941\u0921\u0928\u0947, \u0935\u0932\u0930\u094d\u0935\u093e\u092f\u094d, \u0905\u0921\u093f\u092f\u093e\u0930\u094d <br/> \u092a\u0941\u0926\u0941\u0935\u093e\u0932\u094d \u0935\u0